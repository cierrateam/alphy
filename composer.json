{
    "name": "cierrateam/alphy",
    "description": "Another awesome laravel package",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Vittorio Emmermann",
            "email": "x@vittorio.dev"
        }
    ],
    "require": {
        "php": ">=7.2",
        "illuminate/support": "7.*"
    },
    "require-dev": {
        "psy/psysh": "@stable",
        "phpunit/phpunit": "^9.0",
        "orchestra/testbench": "^5.3",
        "mockery/mockery": "^1.3.1",
        "laravel/framework": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Cierrateam\\Alphy\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\": "vendor/orchestra/testbench-core/laravel/app",
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Cierrateam\\Alphy\\AlphyServiceProvider"
            ]
        }
    }
}